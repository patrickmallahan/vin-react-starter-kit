{
  "name": "vin-javascript-starter-kit",
  "version": "1.0.0",
  "description": "Starter kit for creating new client-side applications at Vinsolutions",
  "main": "main.js",
  "scripts": {
    "start": "gulp",
    "test": "mocha --compilers js:babel/register --recursive --colors",
    "coverage": "istanbul cover --color _mocha -- --compilers js:babel/register --recursive --colors",
    "coverage-es6": "./node_modules/.bin/babel-node ./node_modules/.bin/isparta cover _mocha -- --recursive --colors --reporter progress",
    "open-coverage": "open coverage/lcov-report/index.html"
  },
  "author": "Cory House",
  "license": "ISC",
  "dependencies": {
    "browserify": "^11.0.1",
    "flux": "^2.0.3",
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  },
  "devDependencies": {
    "babel": "^5.8.23",
    "babel-eslint": "^4.1.3",
    "babelify": "^6.3.0",
    "chai": "^3.3.0",
    "del": "^2.0.2",
    "gulp": "^3.9.0",
    "gulp-concat": "^2.6.0",
    "gulp-connect": "^2.2.0",
    "gulp-debug": "^2.1.2",
    "gulp-eslint": "^0.15.0",
    "gulp-mocha": "^2.1.3",
    "gulp-open": "^1.0.0",
    "gulp-sass": "^2.0.4",
    "gulp-shell": "^0.5.0",
    "isparta": "^3.1.0",
    "istanbul": "^0.3.22",
    "minifyify": "^7.1.0",
    "mocha": "^2.3.3",
    "reactify": "^1.1.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  }
